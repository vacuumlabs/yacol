    "babel-preset-node6": "^11.0.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-watch": "^2.0.3-rc0",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "eslint": "^3.7.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.0.0",
    "mocha": "^3.1.2"
